{
    "version": "2.0",
    "extensions": {
        "durableTask": {
            "hubName": "PersistenceDemo",
            "storageProvider": {
                "maxQueuePollingInterval": "00:00:02"
            },
            "maxEntityOperationBatchSize": 1
          }
    },
    "logging": {
        "applicationInsights": {
            "samplingExcludedTypes": "Request",
            "samplingSettings": {
                "isEnabled": true
            }
        }
    }
}